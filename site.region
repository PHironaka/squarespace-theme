<!doctype html>
<html>
  <head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="initial-scale=1">
    <link href="https://fonts.googleapis.com/css?family=Raleway" rel="stylesheet">

    {squarespace-headers}
    <squarespace:script data-preserve-html-node="true" src="site.js" combo="true"/>

  </head>
  <body id="{squarespace.page-id}" class="{squarespace.page-classes}">
<p>Hello, World!</p>
    <header>
      <squarespace:navigation navigationId="mainNav" template="navigation" />

      {.section website}
        <h1>
          <a href="/" data-content-field="site-title">
            {.section logoImageUrl}
             <img src="{logoImageUrl}?format=original" alt="{siteTitle}" />
            {.or}
              {website.siteTitle}
            {.end}
          </a>
        </h1>

        {.section siteTagLine}

        {.end}

      {.end}




    </header>

    <main data-content-field="main-content">
      {squarespace.main-content}
    </main>

    <footer>
      <squarespace:block-field id="footer-blocks" columns="12" />
    </footer>

    <script type="text/javascript" src="/scripts/site-bundle.js"></script>

    {squarespace-footers}

  </body>
</html>
